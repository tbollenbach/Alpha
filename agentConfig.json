{
  "enabled": true,
  "mode": "agent",
  "coordinatorUrl": "ws://127.0.0.1:8080",
  "apiKey": "local-coordinator-agent",
  "reconnectInterval": 5000,
  "capabilities": {
    "cpu": true,
    "gpu": false,
    "memory": true
  },
  "limits": {
    "maxCPUUsage": 80,
    "maxMemoryUsage": 80,
    "maxConcurrentTasks": 1
  },
  "modules": {
    "cpu_compute": true,
    "hash_compute": true,
    "fibonacci": true,
    "prime_check": true,
    "custom_code": false
  }
}

